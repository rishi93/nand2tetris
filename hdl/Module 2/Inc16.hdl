/**
 * 16-bit incrementer:
 * out = in + 1
 */
CHIP Inc16 {
    IN in[16];
    OUT out[16];

    PARTS:
    /*
    Not16(in=in, out=temp);
    Add16(a=temp, b=true, out=temp2);
    Not16(in=temp2, out=out);
    */
    Add16(a=in, b[0]=true, out=out);
}